<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/_coordinator"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/DarkBlue"
	android:fitsSystemWindows="true">
	<com.google.android.material.appbar.AppBarLayout
		android:id="@+id/_app_bar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/DarkBlue"
		android:fitsSystemWindows="true">
		<com.google.android.material.appbar.CollapsingToolbarLayout
			android:id="@+id/collapsingtoolbar1"
			android:layout_width="match_parent"
			android:padding="8dp"
			android:fitsSystemWindows="true"
			app:layout_scrollFlags="scroll|exitUntilCollapsed"
			android:layout_height="?attr/collapsingToolbarLayoutLargeSize"
			app:contentScrim="@color/DarkBlue"
			app:collapsedTitleGravity="left"
			app:titleCollapseMode="scale"
			app:expandedTitleTextAppearance="@style/CustomExpandedTitleText"
			app:collapsedTitleTextAppearance="@style/CustomCollapsedTitleText">
<com.google.android.material.appbar.MaterialToolbar
				android:id="@+id/_toolbar"
				android:layout_width="match_parent"
				android:layout_height="?attr/actionBarSize"
				app:title="Withdraw your money"
				app:layout_collapseMode="pin"
				app:navigationIcon="@drawable/icon_arrow_back_round" />
		</com.google.android.material.appbar.CollapsingToolbarLayout>
	</com.google.android.material.appbar.AppBarLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_behavior="@string/appbar_scrolling_view_behavior">
		<androidx.core.widget.NestedScrollView
			android:id="@+id/vscroll1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			app:layout_behavior="@string/appbar_scrolling_view_behavior"
			android:fillViewport="true"
			android:scrollbars="none">
			<LinearLayout
				android:id="@+id/linear1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<RelativeLayout
					android:id="@+id/withdraw_body"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:background="@color/DarkBlue">
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/important_note"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="20dp"
						android:layout_marginTop="20dp"
						android:layout_marginRight="20dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:clickable="true"
						android:focusable="true"
						android:checkable="true"
						app:cardBackgroundColor="@color/DarkBlue"
						app:strokeColor="@color/LightBlue"
						android:layout_centerHorizontal="true">
						<LinearLayout
							android:id="@+id/linear65"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="horizontal">
							<LinearLayout
								android:id="@+id/linear64"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:padding="3dp"
								android:gravity="center_vertical"
								android:orientation="vertical">
								<ImageView
									android:id="@+id/imageview22"
									android:layout_width="20dp"
									android:layout_height="20dp"
									android:layout_marginLeft="5dp"
									android:layout_marginRight="2dp"
									android:src="@drawable/icon_info"
									android:scaleType="fitXY" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear63"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:orientation="vertical"
								android:layout_gravity="right"
								android:layout_weight="1">
								<TextView
									android:id="@+id/textview17"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingTop="4dp"
									android:paddingRight="8dp"
									android:text="Important Note"
									android:textSize="16sp"
									android:textColor="#64B5F6" />
								<TextView
									android:id="@+id/textview18"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingTop="3dp"
									android:paddingRight="8dp"
									android:paddingBottom="4dp"
									android:text="After withdraw or deposit money will be added to your account within 1 to 5 hours. (Admin Verify)"
									android:textSize="14sp"
									android:textColor="@color/md_theme_dark_outline" />
							</LinearLayout>
						</LinearLayout>
					</com.google.android.material.card.MaterialCardView>
					<com.google.android.material.textfield.TextInputLayout
						android:id="@+id/number_tablayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="30dp"
						android:layout_marginTop="15dp"
						android:layout_marginRight="30dp"
						style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
						android:visibility="visible"
						android:layout_below="@id/methods_listview">
						<EditText
							android:id="@+id/number_txt"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:textSize="16sp"
							android:textColor="#FAFAFA"
							android:hint="Enter your number/id"
							android:textColorHint="#607D8B"
							android:singleLine="true"
							android:imeOptions="actionNext" />
					</com.google.android.material.textfield.TextInputLayout>
					<com.google.android.material.textfield.TextInputLayout
						android:id="@+id/amount_tablayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="30dp"
						android:layout_marginTop="15dp"
						android:layout_marginRight="30dp"
						style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
						android:visibility="visible"
						android:layout_below="@id/number_tablayout">
						<EditText
							android:id="@+id/amount_txt"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:textSize="16sp"
							android:textColor="#FAFAFA"
							android:hint="Enter amount"
							android:textColorHint="#607D8B"
							android:singleLine="true"
							android:inputType="numberDecimal"
							android:imeOptions="actionNext" />
					</com.google.android.material.textfield.TextInputLayout>
					<com.google.android.material.textfield.TextInputLayout
						android:id="@+id/orderid_tablayout"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="30dp"
						android:layout_marginTop="15dp"
						android:layout_marginRight="30dp"
						style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
						android:visibility="visible"
						android:layout_below="@id/amount_tablayout">
						<EditText
							android:id="@+id/orderid_txt"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:textSize="16sp"
							android:textColor="#FAFAFA"
							android:hint="Enter order id/transaction id"
							android:textColorHint="#607D8B"
							android:singleLine="true"
							android:imeOptions="actionDone" />
					</com.google.android.material.textfield.TextInputLayout>
					<Button
						android:id="@+id/apply_button"
						android:layout_width="300dp"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="20dp"
						android:layout_marginRight="10dp"
						android:layout_marginBottom="20dp"
						android:padding="8dp"
						android:text="Apply Now!"
						android:textSize="16sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:layout_below="@id/linear2"
						android:layout_centerHorizontal="true" />
					<LinearLayout
						android:id="@+id/linear2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="20dp"
						android:padding="8dp"
						android:orientation="vertical"
						android:layout_below="@id/orderid_tablayout">
						<TextView
							android:id="@+id/txt"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Withdraw!"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="@color/colorSurface"
							android:layout_gravity="center_horizontal|center_vertical" />
						<LinearLayout
							android:id="@+id/linear88"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginBottom="10dp"
							android:padding="5dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<TextView
								android:id="@+id/money_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="5dp"
								android:text="0.000$"
								android:textSize="16sp"
								android:textColor="#64B5F6" />
							<LinearLayout
								android:id="@+id/linear_equal"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginLeft="10dp"
								android:layout_marginRight="10dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical">
								<com.google.android.material.card.MaterialCardView
									android:id="@+id/equal1"
									android:layout_width="35dp"
									android:layout_height="10dp"
									android:layout_marginBottom="3dp"
									app:cardBackgroundColor="#142342"
									android:orientation="horizontal" />
								<com.google.android.material.card.MaterialCardView
									android:id="@+id/linear36"
									android:layout_width="35dp"
									android:layout_height="10dp"
									app:cardBackgroundColor="#142342"
									android:orientation="horizontal" />
							</LinearLayout>
							<TextView
								android:id="@+id/convert_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="0.000 BDT"
								android:textSize="16sp"
								android:textColor="#64B5F6" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear3"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="1dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview7"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Amount:"
								android:textSize="14sp"
								android:textColor="@color/md_theme_light_primaryContainer" />
							<LinearLayout
								android:id="@+id/linear4"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="1" />
							<TextView
								android:id="@+id/amount_txt2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="2dp"
								android:paddingBottom="8dp"
								android:text="0.000"
								android:textSize="14sp"
								android:textColor="@color/md_theme_light_primaryContainer" />
							<TextView
								android:id="@+id/textview9"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="$"
								android:textSize="16sp"
								android:textColor="@color/md_theme_light_primaryContainer" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear7"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="1dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview13"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Fee:"
								android:textSize="14sp"
								android:textColor="@color/md_theme_light_primaryContainer" />
							<LinearLayout
								android:id="@+id/linear8"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="1" />
							<TextView
								android:id="@+id/fee_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="2dp"
								android:paddingBottom="8dp"
								android:text="0.000"
								android:textSize="14sp"
								android:textColor="@color/md_theme_light_primaryContainer" />
							<TextView
								android:id="@+id/textview15"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="$"
								android:textSize="16sp"
								android:textColor="@color/md_theme_light_primaryContainer" />
						</LinearLayout>
						<com.google.android.material.divider.MaterialDivider
							android:id="@+id/linear11"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal"
							app:dividerInsetStart="16dp"
							app:dividerInsetEnd="16dp" />
						<LinearLayout
							android:id="@+id/linear9"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="1dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview16"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Total:"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="@color/gradient_middle" />
							<LinearLayout
								android:id="@+id/linear10"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="1" />
							<TextView
								android:id="@+id/total_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="2dp"
								android:paddingBottom="8dp"
								android:text="0.000"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="@color/gradient_middle" />
							<TextView
								android:id="@+id/textview19"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="$"
								android:textSize="16sp"
								android:textStyle="bold"
								android:textColor="@color/gradient_middle" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear78"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="1dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/available_main_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Available Balance:"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="@color/colorError" />
							<LinearLayout
								android:id="@+id/linear79"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:orientation="horizontal"
								android:layout_weight="1" />
							<TextView
								android:id="@+id/available_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="2dp"
								android:paddingBottom="8dp"
								android:text="0.000"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="@color/colorError" />
							<TextView
								android:id="@+id/textview38"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="$"
								android:textSize="16sp"
								android:textStyle="bold"
								android:textColor="@color/colorError" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear_selected_Body"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:visibility="gone">
							<TextView
								android:id="@+id/textview39"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Selected"
								android:textSize="18sp"
								android:textColor="@color/md_theme_dark_surfaceTint" />
							<com.google.android.material.card.MaterialCardView
								android:id="@+id/bg"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:layout_marginTop="5dp"
								android:layout_marginBottom="5dp"
								app:cardBackgroundColor="#171E2E"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear67"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="8dp"
									android:orientation="horizontal">
									<de.hdodenhof.circleimageview.CircleImageView
										android:id="@+id/img2"
										android:layout_width="60dp"
										android:layout_height="60dp"
										android:layout_marginTop="5dp"
										android:layout_marginBottom="5dp"
										android:src="@drawable/profile_logo"
										app:civ_border_width="3dp"
										app:civ_border_overlay="true" />
									<LinearLayout
										android:id="@+id/linear90"
										android:layout_width="match_parent"
										android:layout_height="match_parent"
										android:padding="8dp"
										android:orientation="vertical">
										<TextView
											android:id="@+id/money_name2"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:layout_marginTop="5dp"
											android:layout_marginBottom="5dp"
											android:gravity="center_horizontal|center_vertical"
											android:text="USDT Only"
											android:textSize="14sp"
											android:textStyle="bold"
											android:textColor="#FFFFFF" />
										<TextView
											android:id="@+id/receive_id2"
											android:layout_width="wrap_content"
											android:layout_height="wrap_content"
											android:layout_marginTop="5dp"
											android:layout_marginBottom="5dp"
											android:text="00000000000"
											android:textSize="12sp"
											android:textColor="#FFFFFF" />
									</LinearLayout>
								</LinearLayout>
							</com.google.android.material.card.MaterialCardView>
						</LinearLayout>
					</LinearLayout>
					<GridView
						android:id="@+id/methods_listview"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginTop="20dp"
						android:padding="8dp"
						android:numColumns="3"
						android:stretchMode="columnWidth"
						android:layout_below="@id/important_note" />
				</RelativeLayout>
			</LinearLayout>
		</androidx.core.widget.NestedScrollView>
	</LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>